{
    "compilerOptions": {
        "outDir": "./out",
        "lib": [
            "es6"
        ],
        "module": "commonjs",
        "esModuleInterop": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": true,
        "sourceMap": true,
        "strict": true,
        "target": "es6",
        "types": [
            "node",
            "vscode"
        ]
    }
}